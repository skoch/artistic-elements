{
  "name": "artistic-elements",
  "version": "0.2.6-1",
  "description": "artistic web components",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "run-s clean dev",
    "dev": "run-p ts:watch serve",
    "ts:watch": "tsc --watch",
    "serve": "wds --node-resolve --watch",
    "build": "run-s clean bundle",
    "clean": "rimraf dist",
    "bundle": "rollup -c --context this",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick",
    "pre-commit": "pretty-quick --staged"
  },
  "dependencies": {
    "lit": "^2.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@web/dev-server": "^0.1.34",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skoch/artistic-elements.git"
  },
  "keywords": [
    "art",
    "artistic",
    "web",
    "components"
  ],
  "author": "stephen@koch.dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skoch/artistic-elements/issues"
  },
  "homepage": "https://github.com/skoch/artistic-elements#readme"
}
